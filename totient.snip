snippet totient
options head
    intmax_t totient(intmax_t n) {
        intmax_t ret = n;
        for (intmax_t i = 2; i * i <= n; ++i) {
            if (n % i == 0) {
                ret -= ret / i;
                while (n % i == 0) {
                    n /= i;
                }
            }
        }
        if (n > 1) {
            ret -= ret / n;
        }
        return ret;
    }
