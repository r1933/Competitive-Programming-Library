options head
    vector<intmax_t> totientTable(intmax_t n) {
        vector<intmax_t> totient(n + 1);

        for (intmax_t i = 0; i <= n; ++i) {
            totient[i] = i;
        }

        for (intmax_t i = 2; i <= n; ++i) {
            if (totient[i] == i) {
                for (intmax_t j = 0; j <= n; j += i) {
                    totient[j] = totient[j] / i * (i - 1);
                }
            }
        }

        return totient;
    }
