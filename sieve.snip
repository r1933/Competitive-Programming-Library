options head
    vector<bool> sieve(intmax_t MAX) {
        vector<bool> is_prime(MAX + 1, true);
        is_prime[0] = is_prime[1] = false;

        for (intmax_t i = 2; i * i <= MAX; ++i) {
            if (is_prime[i]) {
                for (intmax_t j = 2; i * j <= MAX; ++j) {
                    is_prime[i * j] = false;
                }
            }
        }

        return is_prime;
    }
