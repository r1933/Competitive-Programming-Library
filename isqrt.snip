snippet isqrt
options word
    template <class T>
    T isqrt(T n) {
        if (n <= 1) return n;
        T ok = 1;
        T ng = static_cast<T>(1) << (CHAR_BIT * (sizeof(T) / 2));
        while (ng - ok > 1) {
            const T mid = (ok + ng) >> 1;
            if (mid * mid <= n) ok = mid;
            else ng = mid;
        }
        return ok;
    }
