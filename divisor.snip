snippet divisor
options head
    vector<intmax_t> divisor(intmax_t n) {
        vector<intmax_t> ret;

        for (intmax_t i = 1; i * i <= n; ++i) {
            if (n % i == 0) {
                ret.emplace_back(i);
                if (i * i != n) {
                    ret.emplace_back(n / i);
                }
            }
        }

        sort(ret.begin(), ret.end());
        return ret;
    }
