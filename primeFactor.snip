    map<intmax_t, intmax_t> primeFactor(intmax_t n) {
        map<intmax_t, intmax_t> ret;

        for (intmax_t i = 2; i * i <= n; ++i) {
            while (n % i == 0) {
                ++ret[i];
                n /= i;
            }
        }

        if (n != 1) {
            ret[n] = 1;
        }

        return ret;
    }
